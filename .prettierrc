{
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 120,
  "proseWrap": "never",
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-organize-imports"],
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    },
    {
      "files": "document.ejs",
      "options": {
        "parser": "html"
      }
    }
  ],
  "organizeImportsSkipDestructiveCodeActions": true
}
